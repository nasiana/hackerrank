"""
Climbing Stairs
"""

class Solution:
    def climbStairs(self, n: int) -> int:
        if n == 1:
            return 1
        elif n == 2:
            return 2
        elif n == 3:
            return 3
        return Solution.climbStairs(self, n-2) + Solution.climbStairs(self, n-1)
        
        
