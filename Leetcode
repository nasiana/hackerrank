"""
Two Sum
"""

# My solution

class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        nums_map = {}
        for i in range(len(nums)):
            val = target-nums[i]
            if val in nums_map:
                return(i, nums_map[val])
            nums_map[nums[i]] = i 
